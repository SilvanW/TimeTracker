    {
        "id": "47e920b05b0452a3",
        "type": "function",
        "z": "ab09c729ddbc5d2b",
        "name": "detailRequest",
        "func": "const filter = flow.get(\"filter\")\n\n// Query without filter\nmsg.query = `\nselect tbltime.id, tblprojects.name as \"project_name\", \"start\", \"end\", extract(epoch from \"end\"-\"start\") / 3600 as \"working_time\", extract (week from start) as \"calendar_week\"\nfrom tbltime\nleft join tblprojects on tblprojects.id = tbltime.project_id`\n\nif (filter === undefined) {\n    return msg;\n}\n\nif (filter[\"calendar_week\"] === null) {\n    return msg;\n}\n\nmsg.query = `\nselect tbltime.id, tblprojects.name as \"project_name\", \"start\", \"end\", extract(epoch from \"end\"-\"start\") / 3600 as \"working_time\", extract (week from start) as \"calendar_week\"\nfrom tbltime\nleft join tblprojects on tblprojects.id = tbltime.project_id\nwhere extract (week from start) = ${filter[\"calendar_week\"]}`\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 360,
        "y": 160,
        "wires": [
            [
                "ae8a9246450d3bc7"
            ]
        ]
    },
    {
        "id": "c8936184f999a438",
        "type": "function",
        "z": "ab09c729ddbc5d2b",
        "name": "overviewRequest",
        "func": "const grouping = flow.get(\"grouping\")\nmsg.query = `\nselect calendar_week AS KW, sum(working_time) AS Time\nfrom (\n    select tbltime.id, tblprojects.name as \"project_name\", \"start\", \"end\", extract(epoch from \"end\"-\"start\") / 3600 as \"working_time\", extract (week from start) as \"calendar_week\"\n    from tbltime left join tblprojects on tblprojects.id = tbltime.project_id\n)\ngroup by calendar_week order by calendar_week DESC\n`\n\nif (grouping === 1) {\n    msg.query = `\n    select \"day_of_year\" AS DOY, sum(\"working_time\") AS Time\n    from (\n        select tbltime.id, tblprojects.name as \"project_name\", \"start\", \"end\", extract(epoch from \"end\"-\"start\") / 3600 as \"working_time\", extract (DOY from start) as \"day_of_year\"\n        from tbltime left join tblprojects on tblprojects.id = tbltime.project_id\n    )\n    group by \"day_of_year\" order by \"day_of_year\" DESC\n    `\n}\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 370,
        "y": 80,
        "wires": [
